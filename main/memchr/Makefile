NAME_PROGRAM  = memchr
NAME_C = ft_memchr.c

all:
		@gcc -Wall -Wextra -Werror main_$(NAME_PROGRAM).c \
		-L/Users/cjosue/Desktop/Libft/ -lft \
		-I /Users/cjosue/Desktop/Libft/ \
		-o $(NAME_PROGRAM)
